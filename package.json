{
  "name": "sosol-backend",
  "version": "1.0.0",
  "description": "SOSOL backend with Prisma + Graphql",
  "main": "src/server.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "aws:s3:ls": "aws --endpoint-url=http://localhost:4566 s3 ls s3://sosol-dev",
    "aws:s3:mb": "aws --endpoint-url=http://localhost:4566 s3 mb s3://sosol-dev",
    "dev": "nodemon -e js,graphql src/server.js",
    "docker:bash": "docker exec -it backend_sosol-api_1 sh",
    "docker:build:dev": "bash ./script/docker-dev.sh",
    "docker:prisma": "docker exec -it backend_sosol-api_1 npm run prisma:dev",
    "prisma:dev": "npx prisma migrate dev --name init",
    "prisma:gen": "npx prisma generate",
    "start": "node src/server.js"
  },
  "author": "harkl",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.28.0",
    "@aws-sdk/s3-request-presigner": "^3.28.0",
    "@prisma/client": "^2.30.2",
    "bs58": "^4.0.1",
    "cuid": "^2.1.8",
    "dotenv": "^10.0.0",
    "graphql-tools": "^8.2.0",
    "graphql-yoga": "^1.18.3",
    "haikunator": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "prisma-client-lib": "^1.34.12",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "prisma": "^2.30.2"
  }
}
